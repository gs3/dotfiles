#!/usr/local/bin/zsh

brew_update () {
  brew update
  brew upgrade
  brew cleanup

  cd $HOME/src/github.com/ravelll/dotfiles
  brew bundle dump --force
}

node_up () {
  nodebrew selfupdate
  nodebrew install-binary latest
}

anyenv_update () {
  anyenv update
}

main () {
  brew_update
  anyenv_update
}

main
